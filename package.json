{
  "name": "vectored",
  "version": "0.3.0",
  "description": "A JavaScript 2 and 3D vector math library",
  "main": "vec.js",
  "scripts": {
    "lint": "eslint --fix --ext .js .",
    "uglify": "minify index.js --out-file build/vec.min.js",
    "build": "npm run lint && browserify index.js > build/vec.js && minify build/vec.js --out-file build/vec.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Vuurvos1/vectored.git"
  },
  "keywords": [
    "vector",
    "2d",
    "3d",
    "math"
  ],
  "author": "Sam de Kanter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Vuurvos1/vectored/issues"
  },
  "homepage": "https://github.com/Vuurvos1/vectored#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-minify": "^0.5.1",
    "browserify": "^17.0.0",
    "eslint": "^7.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "uglify-js": "^3.10.1"
  }
}
